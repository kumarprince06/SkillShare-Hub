{
  "version": 2,
  "name" : "SkillShare Hub",
  "builds": [
      {
          "src": "./index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/index.html", 
          "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      }
  ]
}